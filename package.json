    "mocha": "^8.2.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.0.2"
  },
  "keywords": [
    "parser",
    "tokenizer",
    "tokens",
    "code",
    "javascript"
  ],
  "files": [
    "src",
    "dist"
  ],

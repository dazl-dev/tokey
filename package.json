{
  "name": "toky",
  "description": "simple code like tokenizer",
  "version": "0.0.6",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha -r @ts-tools/node/r test/**/*.spec.ts",
    "test:mocha": "mocha -r @ts-tools/node/r",
    "typecheck": "tsc --noEmit",
